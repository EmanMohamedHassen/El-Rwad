                var _audio = new Audio("/Alarm.wav");
                _audio.play();
                var tmpl = ` <div class="post in">
                                              <img class="avatar" alt="" src="../assets/layouts/layout/img/avatar2.jpg" />
                                              <div class="message">
                                                  <span class="arrow"></span>
                                                  <a href="javascript:;" class="name">${obj.username}</a>
                                                  <span class="datetime">${new Date().getHours()}:${new Date().getMinutes()}</span>
                                                  <span class="body"> ${obj.msg} </span>
                                              </div>
                                          </div>`;
                $("#div-messages").append(tmpl);


                $("#" + obj.to).html("1");
                $("#btnSendMsg").val("");
            }



            chat.client.lobbyEntered = (allUsers) => {

                $("#span-users-count").html(allUsers.length);


                var tmpl = "";
                $.each(allUsers, (index, username) => {

                    if (username == '@User.Identity.Name') {

                    }
                    else {
                        tmpl += `   <li class="media" data-username=${username}>
                                  <div class="media-status">
                                      <span id=${username} class="badge badge-success" data-username=${username}></span>
                                  </div>
                                  <img class="media-object" src="../assets/layouts/layout/img/avatar3.jpg" alt="...">
                                  <div class="media-body">
                                      <h4 class="media-heading">${username}</h4>
                                      <div class="media-heading-sub"> Project Manager </div>
                                  </div>
                              </li>`;
                    }


                });

                $("#div-online-users").html(tmpl);
                $(".media").click(function () {
                    var username = $(this).data("username");
                    $("#hfUsername").val(username);
                    $("#quick_sidebar_tab_1").addClass("page-quick-sidebar-content-item-shown")
                });
            };
            $("#txtMsg").keydown((e) => {

                if (e.keyCode == 13) {
                    var msg = $("#txtMsg").val();
                    var tmpl = `<div class="post out"> <img class="avatar" alt="" src="../assets/layouts/layout/img/avatar3.jpg" />
                                              <div class="message">
                                                  <span class="arrow"></span>
                                                  <a href="javascript:;" class="name">@User.Identity.Name</a>
                                                  <span class="datetime">${new Date().getHours()}:${new Date().getMinutes()}</span>
                                                  <span class="body"> ${msg} </span>
                                              </div>
                                          </div>`;
                    $("#div-messages").append(tmpl);

                    chat.server.sendChatMessage($("#hfUsername").val(), msg);
                    $("#txtMsg").val("");
                }
            });

            $("#btnSendMsg").click(() => {
                var msg = $("#txtMsg").val();
                var tmpl = `<div class="post out"> <img class="avatar" alt="" src="../assets/layouts/layout/img/avatar3.jpg" />
                                              <div class="message">
                                                  <span class="arrow"></span>
                                                  <a href="javascript:;" class="name">@User.Identity.Name</a>
                                                  <span class="datetime">${new Date().getHours()}:${new Date().getMinutes()}</span>
                                                  <span class="body"> ${msg} </span>
                                              </div>
                                          </div>`;
                $("#div-messages").append(tmpl);

                chat.server.sendChatMessage($("#hfUsername").val(), msg);
                $("#txtMsg").val("");
            });
            $.connection.hub.start().done(function () {
                console.log($.connection);
            });
        });







